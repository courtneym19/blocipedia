<h1>Edit Wiki</h1>

 <div class="row">
   <div class="col-md-8">
     <%= render partial: 'form', locals: { wiki: @wiki } %>
     <% if current_user.admin? || @wiki.user == current_user || @wiki.collaborators.include?(current_user) %>
       <%= link_to "Manage Collaborators", wiki_collaborators_path(@wiki), class: "btn btn-info" %>
     <% end %>
   </div>
 </div>
